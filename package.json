{
  "name": "chromium-dev-kit",
  "displayName": "Chromium Dev Kit",
  "description": "Development toolkit for Chromium projects with window customization: banner comments, header guards, .mojom/.idl support, and window color & name management",
  "version": "1.0.0",
  "publisher": "ScottLeung",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other",
    "Themes"
  ],
  "activationEvents": [
    "onStartupFinished",
    "onLanguage:cpp"
  ],
  "main": "./dist/extension.js",
  "files": [
    "dist/**",
    "node_modules/@vscode/sqlite3/lib/**",
    "node_modules/@vscode/sqlite3/build/Release/vscode-sqlite3.node",
    "node_modules/@vscode/sqlite3/LICENSE",
    "node_modules/@vscode/sqlite3/package.json",
    "CHANGELOG.md",
    "README.md",
    "logo.png",
    "LICENSE.txt"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/scott-leung/ChromiumDevKit-VSCode"
  },
  "icon": "logo.png",
  "contributes": {
    "commands": [
      {
        "command": "chromiumDevKit.openWindowColorSettings",
        "title": "Chromium Dev Kit: Window Color & Name Settings"
      },
      {
        "command": "chromiumDevKit.applyTemplate1",
        "title": "Chromium Dev Kit: Apply Template 1"
      },
      {
        "command": "chromiumDevKit.applyTemplate2",
        "title": "Chromium Dev Kit: Apply Template 2"
      },
      {
        "command": "chromiumDevKit.applyTemplate3",
        "title": "Chromium Dev Kit: Apply Template 3"
      },
      {
        "command": "chromiumDevKit.applyTemplate4",
        "title": "Chromium Dev Kit: Apply Template 4"
      },
      {
        "command": "chromiumDevKit.applyTemplate5",
        "title": "Chromium Dev Kit: Apply Template 5"
      },
      {
        "command": "chromiumDevKit.applyTemplate6",
        "title": "Chromium Dev Kit: Apply Template 6"
      },
      {
        "command": "chromiumDevKit.applyTemplate7",
        "title": "Chromium Dev Kit: Apply Template 7"
      },
      {
        "command": "chromiumDevKit.applyTemplate8",
        "title": "Chromium Dev Kit: Apply Template 8"
      },
      {
        "command": "chromiumDevKit.applyTemplate9",
        "title": "Chromium Dev Kit: Apply Template 9"
      },
      {
        "command": "chromiumI18n.rebuildIndex",
        "title": "Chromium I18n: Rebuild i18n Index",
        "category": "Chromium I18n"
      },
      {
        "command": "chromiumI18n.aiTranslate",
        "title": "Chromium I18n: AI Translate to Target Language",
        "category": "Chromium I18n"
      },
      {
        "command": "chromiumI18n.search",
        "title": "Chromium I18n: Search i18n Strings",
        "category": "Chromium I18n"
      },
      {
        "command": "chromiumI18n.detectUnused",
        "title": "Chromium I18n: Detect Unused IDS",
        "category": "Chromium I18n"
      },
      {
        "command": "chromiumI18n.openDashboard",
        "title": "Chromium I18n: Open i18n Dashboard",
        "category": "Chromium I18n"
      },
      {
        "command": "chromiumI18n.showDiff",
        "title": "Chromium I18n: Show Translation Diff",
        "category": "Chromium I18n"
      },
      {
        "command": "chromiumI18n.jumpToDefinition",
        "title": "Chromium I18n: Jump to Definition",
        "category": "Chromium I18n"
      },
      {
        "command": "chromiumI18n.jumpToTranslation",
        "title": "Chromium I18n: Jump to Translation",
        "category": "Chromium I18n"
      },
      {
        "command": "chromiumI18n.toggleOverlay",
        "title": "Chromium I18n: Toggle Translation Overlay",
        "category": "Chromium I18n"
      },
      {
        "command": "chromiumI18n.changeOverlayLanguage",
        "title": "Chromium I18n: Change Overlay Language",
        "category": "Chromium I18n"
      },
      {
        "command": "chromiumI18n.createGrdMessage",
        "title": "Chromium I18n: Create GRD Message",
        "category": "Chromium I18n"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "chromiumI18n.createGrdMessage",
          "when": "resourceExtname == .grd || resourceExtname == .grdp",
          "group": "chromium-dev-kit@1"
        }
      ],
      "editor/context": [
        {
          "command": "chromiumI18n.createGrdMessage",
          "when": "resourceExtname == .grd || resourceExtname == .grdp",
          "group": "chromium-dev-kit@1"
        }
      ]
    },
